                        <h4 class="title">
                            <span class="title-menu">Categories</span>
                            <span class="@(ViewContext.RouteData.Values["Action"].ToString() == "Index" ? "btn-open-mobile pull-right" : "btn-open-mobile pull-right home-page")"><i class="fa fa-bars"></i></span>
                            @*<span class="btn-open-mobile pull-right"><i class="fa fa-bars"></i></span>*@
                        </h4>
                        <div class="vertical-menu-content is-home">
                            @foreach (var Mainitem in Model)
                            {
                                foreach (var item in Mainitem.listCategoryEntity.Where(x => x.DisplayOrder == 1).ToList())
                                {
                                    <ul class="vertical-menu-list">
                                        <li>
                                            <a class="parent" href="#">
                                                <img class="icon-menu" alt="Funky roots" src="@Html.DisplayFor(modelItem => item.CategoryPicture)"> @Html.DisplayFor(modelItem => item.CategoryName)
                                            </a>
                                            <div class="vertical-dropdown-menu">
                                                <div class="vertical-groups col-sm-12">

                                                    @foreach (var SubCat in @item.listSubCategoryEntity)
                                                    {
                                                        <div class="mega-group col-sm-4">

                                                            <h4 class="mega-group-header"><span>@Html.DisplayFor(modelItem => SubCat.SubCategoryName)</span></h4>
                                                            @foreach (var SubCatitem in SubCat.listSubCategoryItemEntity)
                                                            {

                                                                <ul class="group-link-default">
                                                                    <li><a href="@Url.Action("Products","Home")">@Html.DisplayFor(modelItem => SubCatitem.SubCategoryItemName)</a></li>
                                                                </ul>
                                                            }


                                                        </div>
                                                    }
                                                    <div class="mega-custom-html col-sm-12">
                                                        <a href="#"><img src="assets/data/banner-megamenu.jpg" alt="Banner"></a>
                                                    </div>
                                                </div>
                                            </div>
                                        </li>
                                    </ul>
                                }

                            }
